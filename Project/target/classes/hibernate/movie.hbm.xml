<hibernate-mapping package="entity">
    <class name="Movie" table="movie">
        <id name="id" column="id"/>
        <property name="maoyanId" column="maoyan_id"/>
        <property name="taobaoId" column="taobao_id"/>
        <property name="gewalaId" column="gewala_id"/>
        <property name="name" column="name"/>
        <property name="description" column="description"/>
        <property name="director" column="director"/>
        <property name="star" column="star"/>
        <property name="is3d" column="3d"/>
        <property name="isIMax" column="imax"/>
        <property name="imgUrl" column="imgUrl"/>
    </class>
</hibernate-mapping>