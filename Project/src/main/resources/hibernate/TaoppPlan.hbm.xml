<hibernate-mapping package="entity">
    <class name="TaoppPlan" table="taobao_plan">
        <id name="id" column="id"><generator class="native" /></id>
        <property name="cId" column="c_Id"/>
        <property name="mId" column="m_Id"/>
        <property name="cName" column="c_name"/>
        <property name="mName" column="m_name"/>
        <property name="date" column="date"/>
        <property name="start" column="start"/>
        <property name="end" column="end"/>
        <property name="th" column="th"/>
        <property name="price" column="price"/>
    </class>
</hibernate-mapping>